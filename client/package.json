{
  "name": "zoe",
  "version": "0.0.0",
  "main": "gulpfile.js",
  "devDependencies": {
    "browserify": "~3.41.0",
    "browserify-shim": "^3.4.1",
    "gulp": "~3.6.0",
    "gulp-bower": "0.0.4",
    "gulp-concat": "^2.2.0",
    "gulp-jshint": "~1.5.3",
    "gulp-minify-css": "^0.3.1",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "~0.2.1",
    "vinyl-source-stream": "~0.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mgbelisle/zoe.git"
  },
  "author": "webmaster@gotozoe.org",
  "bugs": {
    "url": "https://github.com/mgbelisle/zoe/issues"
  },
  "homepage": "http://www.gotozoe.org/",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "angular": "./static/bower_components/angular/angular.js",
    "angular-route": "./static/bower_components/angular-route/angular-route.js",
    "bootstrap": "static/bower_components/bootstrap/dist/js/bootstrap.js",
    "jquery": "./static/bower_components/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular",
      "depends": [
        "jquery"
      ]
    },
    "angular-route": {
      "exports": "ngRoute",
      "depends": [
        "angular"
      ]
    },
    "bootstrap": {
      "depends": [
        "jquery"
      ]
    },
    "jquery": "$"
  }
}
